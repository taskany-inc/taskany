{
  "name": "@taskany/server",
  "description": "Taskany GraphQL Server",
  "version": "0.1.0",
  "main": "dist/index.js",
  "license": "MIT",
  "repository": {
    "url": "git@github.com:taskany-inc/taskany.git"
  },
  "scripts": {
    "postinstall": "npm-run-all types:*",
    "test": "jest",
    "dev": "ts-node-dev --no-notify --respawn --transpile-only -r dotenv/config src/index.ts",
    "types:prisma": "npx prisma generate",
    "types:graphql": "ts-node ./scripts/generateSchema",
    "build": "rm -rf ./dist && npm-run-all types:* && tsc"
  },
  "dependencies": {
    "@prisma/client": "2.19.0",
    "apollo-server-express": "2.21.1",
    "body-parser": "1.19.0",
    "class-validator": "0.13.1",
    "express": "4.17.1",
    "graphql": "15.5.0",
    "jsonwebtoken": "8.5.1",
    "passport": "0.4.1",
    "passport-jwt": "4.0.0",
    "prisma": "2.18.0",
    "reflect-metadata": "0.1.13",
    "type-graphql": "1.1.1",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@types/graphql": "14.5.0",
    "@types/graphql-fields": "1.3.3",
    "@types/jest": "26.0.20",
    "@types/node": "12.20.4",
    "@types/passport": "1.0.6",
    "@types/passport-jwt": "3.0.5",
    "@types/ws": "7.4.0",
    "apollo-server-testing": "2.21.0",
    "dbdocs": "0.4.0",
    "dotenv": "8.2.0",
    "graphql-fields": "2.0.3",
    "graphql-scalars": "1.8.0",
    "jest": "26.6.3",
    "jest-environment-node": "26.6.2",
    "nanoid": "3.1.20",
    "npm-run-all": "4.1.5",
    "pg": "8.5.1",
    "prisma-dbml-generator": "0.4.0",
    "ts-jest": "26.5.3",
    "ts-node": "9.1.1",
    "ts-node-dev": "1.1.1",
    "typegraphql-prisma": "0.11.1",
    "typescript": "4.2.2"
  }
}
