{
  "name": "@taskany/server",
  "description": "Taskany GraphQL Server",
  "version": "0.1.0",
  "main": "dist/index.js",
  "license": "MIT",
  "repository": {
    "url": "git@github.com:productivity-tools/taskany.git"
  },
  "scripts": {
    "postinstall": "npm run types",
    "types": "npm-run-all types:*",
    "test": "jest",
    "dev": "ts-node-dev --no-notify --respawn --transpile-only -r dotenv/config src/index.ts",
    "types:prisma": "npx prisma generate",
    "types:graphql": "ts-node ./scripts/generateSchema",
    "build": "rm -rf ./dist && npm run types && tsc"
  },
  "dependencies": {
    "@prisma/client": "2.25.0",
    "apollo-server-express": "2.25.2",
    "class-validator": "0.13.1",
    "express": "4.17.1",
    "graphql": "15.5.1",
    "jsonwebtoken": "8.5.1",
    "passport": "0.4.1",
    "passport-jwt": "4.0.0",
    "prisma": "2.25.0",
    "reflect-metadata": "0.1.13",
    "type-graphql": "1.1.1",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@types/graphql": "14.5.0",
    "@types/graphql-fields": "1.3.3",
    "@types/jest": "26.0.23",
    "@types/node": "15.12.5",
    "@types/passport": "1.0.6",
    "@types/passport-jwt": "3.0.5",
    "@types/ws": "7.4.5",
    "apollo-server-testing": "2.25.2",
    "dotenv": "10.0.0",
    "graphql-fields": "2.0.3",
    "graphql-scalars": "1.10.0",
    "jest": "27.0.6",
    "jest-environment-node": "27.0.6",
    "nanoid": "3.1.23",
    "npm-run-all": "4.1.5",
    "pg": "8.6.0",
    "ts-jest": "27.0.3",
    "ts-node": "10.0.0",
    "ts-node-dev": "1.1.6",
    "typegraphql-prisma": "0.14.4",
    "typescript": "4.2.2"
  }
}
